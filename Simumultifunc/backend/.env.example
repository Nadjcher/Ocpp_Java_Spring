# GPM Dry-Run Configuration 
# Copy this file to .env and fill in your credentials 
 
# Enable/disable GPM dry-run mode 
GPM_ENABLED=true 
 
# TTE Energy Service Manager API 
GPM_BASE_URL=https://evplatform.evcharge-test.totalenergies.com/apigw/energy-service-manager 
 
# OAuth2 Cognito Authentication 
GPM_TOKEN_URL=https://tte-pool-prod.auth.eu-central-1.amazoncognito.com/oauth2/token 
GPM_CLIENT_ID=your-client-id-here 
GPM_CLIENT_SECRET=your-client-secret-here 
 
# Simulation defaults 
GPM_TICK_INTERVAL=15 
GPM_NUMBER_OF_TICKS=96 
GPM_API_TIMEOUT=30000 
GPM_RETRY_DELAY=1000
